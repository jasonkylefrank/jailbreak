{
  "name": "jailbreak",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "firebase-emulate": "firebase emulators:start  --import=./emulator-data  --export-on-exit",
    "next-dev": "next dev",

    "dev": "npm-run-all --parallel  firebase-emulate  next-dev",

    "dev:static": "serve ./out",
    "dev:prod-backend": "next dev",
    "build": "next build && next export && npm run lint:functions",
    "start": "next start",
    "lint": "next lint",
    "lint:functions": "cd functions && npm run lint"
  },
  "dependencies": {
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.4.2",
    "@mui/material": "^5.4.3",
    "@mui/styled-engine-sc": "^5.4.2",
    "firebase": "^9.6.7",
    "next": "12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-firebase-hooks": "^5.0.3",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "eslint": "8.9.0",
    "eslint-config-next": "12.1.0",
    "npm-run-all": "^4.1.5"
  }
}
